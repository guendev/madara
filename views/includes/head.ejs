    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-130681925-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());
        gtag('config', 'UA-130681925-1');
    </script>

    <% if (showAds) {%>
        <!-- Google Adsense -->
        <script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-4136501395952974" crossorigin="anonymous"></script>

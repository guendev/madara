    <meta property="og:title" content="<%= title %> | <%= process.env.APP %>">
    <meta property="og:description" content="<%= description %>">
    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="@<%= title %> | <%= process.env.APP %>">
    <meta name="twitter:title" content="<%= title %> | <%= process.env.APP %>">
    <meta name="twitter:description" content="<%= description %>">
    <meta name="twitter:image" content="<%= seo_image %>">
    <meta data-hid="fb:app_id" property="fb:app_id" content="155915046224033">

    <!-- favicon -->

    <link rel="shortcut icon" href="/images/favicon/favicon.ico">
    <link rel="icon" type="image/x-icon" sizes="16x16 32x32" href="/images/favicon/favicon.ico">
    <link rel="apple-touch-icon" sizes="152x152" href="/images/favicon/favicon-152-precomposed.png">

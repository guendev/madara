<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--Meta Seo-->
    <title><%= title %> | <%= process.env.APP %></title>
    <meta name="description" content="<%= description %>">
    <meta name="generator" content="@dnstylish">

    <meta property="og:image" content="<%= seo_image %>">
    <meta property="og:site_name" content="<%= title %> | <%= process.env.APP %>">
